#include<string.h>
int main()
{
	char s[20];
	int i,v=0,c=0;
	printf("Enter string:");
	gets(s);
	for(i=0;s[i]!='\0';i++)
	{
		if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u')
		{
			v++;
		}
		else
		{
			c++;
		}
	}
	printf("\n Number of vowels=%d",v);
	printf("\n number of consonant=%d",c);
	
}
